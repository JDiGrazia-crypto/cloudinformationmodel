            "$ref": "../ContactPoint/schema.json#/definitions/ContactPointPhone"
          }
        },
        "stateProvinceName": {
          "type": "string"
        },
        "leadStatus": {
          "type": "string"
        },
        "photoURL": {
          "type": "string"
        },
        "countryName": {
          "type": "string"
        },
        "partyRole": {
          "type": "string"
        },
        "convertedDate": {
          "type": "string",
          "format": "date"
        },
        "description": {
          "type": "string"
        },
        "id": {
          "type": "string"
        },
        "contactPointEmail": {
          "type": "array",
          "items": {
            "$ref": "../ContactPoint/schema.json#/definitions/ContactPointEmail"
          }
        },
        "companyName": {
          "type": "string"
        },
        "streetName": {
          "type": "string"
        },
        "leadScore": {
          "type": "integer"
        },
        "mobileContactPhone": {
          "type": "array",
          "items": {
            "$ref": "../ContactPoint/schema.json#/definitions/ContactPointPhone"
          }
        },
