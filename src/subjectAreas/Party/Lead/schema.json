        },
        "geoCodeAccuracy": {
          "type": "integer"
        },
        "leadSource": {
          "type": "string"
        },
        "convertedToAccount": {
          "type": "array",
          "items": {
            "$ref": "../Account/schema.json#/definitions/Account"
          }
