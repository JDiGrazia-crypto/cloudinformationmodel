        "annualRevenue": {
          "type": "integer"
        },
        "emailBouncedReason": {
          "type": "string"
        },
        "website": {
          "type": "string"
        },
        "isConverted": {
          "type": "boolean"
        },
        "partnerAccount": {
          "type": "array",
          "items": {
            "$ref": "../Account/schema.json#/definitions/Account"
          }
        }
