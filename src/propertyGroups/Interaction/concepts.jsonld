    {
      "@id": "mailingAddress",
      "@type": "Property",
      "description": "Reference to Mailing address",
      "name": "Mailing Address",
      "range": "ContactPointAddress"
    },
    {
      "@id": "otherContactAddress",
      "@type": "Property",
