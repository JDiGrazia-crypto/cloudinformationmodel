{
  "@context": "http://cloudinformationmodel.org/context.jsonld",
  "@id": "InteractionPropertyGroup",
  "name": "Interaction Property Group",
  "@type": "PropertyGroup",
  "version": "0.1",
  "propertyConcepts": [
    {
      "@id": "contactEmail",
      "@type": "Property",
      "description": "Reference to email address for the contact.",
      "name": "Contact Email",
      "range": "ContactPointEmail"
    },
    {
      "@id": "homePhone",
      "@type": "Property",
      "description": "Reference to home telephone number for the contact.",
      "name": "Home Phone",
      "range": "ContactPointPhone"
    },
    {
      "@id": "businessPhone",
      "@type": "Property",
      "description": "Reference to telephone number for the contact",
      "name": "Business Phone",
      "range": "ContactPointPhone"
    },
    {
      "@id": "faxPhone",
      "@type": "Property",
      "description": "Reference to fax number for the contact",
      "name": "Fax Phone",
      "range": "ContactPointPhone"
    },
    {
      "@id": "mobilePhone",
      "@type": "Property",
      "description": "Reference to Contacts' mobile phone number.",
      "name": "Mobile Phone",
      "range": "ContactPointPhone"
