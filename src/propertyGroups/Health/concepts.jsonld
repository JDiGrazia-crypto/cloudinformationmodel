{
  "@context": "http://cloudinformationmodel.org/context.jsonld",
  "@id": "HealthPropertyGroup",
  "name": "Health Property Group",
  "@type": "PropertyGroup",
  "version": "0.1",
  "propertyConcepts": [
    {
      "@id": "mainDietaryHabitType",
      "@type": "Property",
      "description": "Dietary habit e.g. kosher food eater",
      "name": "Main Dietary Habit Type",
      "range": "DataType"
    },
    {
      "@id": "mainDisabilityType",
      "@type": "Property",
      "description": "Disability or Impairment e.g. wheel chair bound",
      "name": "Main Disability Type",
      "range": "DataType"
    },
    {
      "@id": "personHeight",
      "@type": "Property",
      "description": "How tall is the person",
      "name": "Person Height",
      "range": "DataType"
    },
    {
      "@id": "personHeightUnitOfMeasure",
      "@type": "Property",
      "description": "What unit of measure is used for height",
      "name": "Person Height Unit Of Measure",
      "range": "DataType"
    },
    {
      "@id": "personWeight",
      "@type": "Property",
      "description": "How heavy is the person",
      "name": "Person Weight",
      "range": "DataType"
    },
    {
      "@id": "personWeightUnitOfMeasure",
      "@type": "Property",
      "description": "What unit of measure is used for weight",
      "name": "Person Weight Unit Of Measure",
      "range": "DataType"
    },
    {
      "@id": "isAlcoholConsumer",
      "@type": "Property",
