      "range": "DataType"
    },
    {
      "@id": "hasDrugAbuseHistory",
      "@type": "Property",
      "description": "Indicator whether individual had a history of drug abuse",
      "name": "Has Drug Abuse History",
      "range": "DataType"
    },
    {
      "@id": "isTobaccoConsume",
      "@type": "Property",
      "description": "Indicator whether individual had a history of drug abuse",
      "name": "Is Tobacco Consumer",
      "range": "DataType"
    },
    {
      "@id": "hospitalizationsLast5YearsCount",
      "@type": "Property",
      "description": "How many times the individual been hospitalized last 5 years",
      "name": "Hospitalizations Last 5 Years Count",
      "range": "DataType"
    },
    {
      "@id": "surgeriesLast5YearsCount",
      "@type": "Property",
      "description": "How many times the individual had any type of surgery in last 5 years",
      "name": "Surgeries Last 5 Years Count",
      "range": "DataType"
    }
